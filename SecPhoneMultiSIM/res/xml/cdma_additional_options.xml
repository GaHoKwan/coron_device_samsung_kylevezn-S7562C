<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceScreen android:title="@string/callsettings_call_barring" android:key="voicecall_barring_key">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.CdmaCallBarring" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/labelCW" android:key="button_cw_key">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.CdmaCallWaitingSetting" />
    </PreferenceScreen>
    <CheckBoxPreference android:title="@string/voice_privacy" android:key="voiceprivacy_preference" android:summary="@string/voice_privacy_summary" />
    <PreferenceScreen android:persistent="false" android:title="@string/fdn" android:key="button_fdn_key" android:summary="@string/sum_fdn">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.FdnSetting" />
    </PreferenceScreen>
    <com.android.phone.callsettings.SwitchDialogPreference android:title="@string/callsettings_areacode_title" android:key="button_areacode_key" android:summary="@string/callsettings_areacode_summary_off" android:defaultValue="false" />
    <CheckBoxPreference android:title="@string/callsettings_noise_reduction_title" android:key="button_noise_reduction_key" android:summary="@string/callsettings_noise_reduction_summary" />
    <PreferenceCategory android:persistent="false" android:title="@string/category_voiceCallEqSettings" android:key="button_voicecall_eq_category_key">
        <ListPreference android:persistent="false" android:entries="@array/voicecall_eq_list_entries" android:title="@string/menu_voiceCallEqListSetting" android:key="voicecall_eq_list_setting" android:summary="@string/summary_voiceCallEqListSetting" android:entryValues="@array/voicecall_eq_list_values" />
        <PreferenceScreen android:enabled="true" android:title="@string/menu_voiceCallEqAnalysisSetting" android:key="voicecall_eq_analysis_setting" android:summary="@string/summary_voiceCallEqAnalysisSetting">
            <intent android:action="com.sec.hearingadjust.launch" />
        </PreferenceScreen>
    </PreferenceCategory>
</PreferenceScreen>